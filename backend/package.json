{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "exports": "./dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.16"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "run-script-os",
    "build:darwin:linux": "./node_modules/typescript/bin/tsc && ./node_modules/.bin/copyfiles -u 1 ./src/**/routes/swagger.yaml dist/",
    "build:win32": ".\\node_modules\\.bin\\tsc.cmd &&  .\\node_modules\\.bin\\copyfiles -u 1 ./src/**/routes/swagger.yaml dist/",
    "start": "run-script-os",
    "start:darwin:linux": "node -r dotenv/config ./dist/index dotenv_config_path=./env/prod.env",
    "start:win32": "node -r dotenv/config .\\dist\\index dotenv_config_path=.\\env\\prod.env",
    "start:dev": "run-script-os",
    "start:dev:darwin:linux": "node -r dotenv/config ./dist/index dotenv_config_path=./env/dev.env",
    "start:dev:win32": "node -r dotenv/config .\\dist\\index dotenv_config_path=.\\env\\dev.env",
    "lint": "run-script-os",
    "lint:darwin:linux": "./node_modules/eslint/bin/eslint.js -c .eslintrc.json --ext .ts src",
    "lint:win32": ".\\node_modules\\.bin\\eslint.cmd -c .eslintrc.json --ext .ts src"
  },
  "author": "Kristjan Cuznar",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "mqtt": "^5.6.2",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^10.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mqtt": "^2.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.14.0",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "openapi-typescript": "^6.7.5",
    "prettier": "^3.0.3",
    "run-script-os": "^1.1.6",
    "typescript": "^5.2.2"
  }
}
